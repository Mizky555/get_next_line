gcc get_next_line.c -d BUFFER_SIZE=5

int main(void)
{
	int	fd;
	char	*dog;

	fd = open("opal_ii"); //file descriptor

	//first line
	dog = get_next_line(fd);
	printf("dog = %s\n", dog);
	
	//cleanup
	free(dog);
	dog = null;

	//second line
	dog = get_next_line(fd);
	printf("dog = %s\n", dog);

	//cleanup
	free(dog)
	dog = null;

	close(fd);
}
